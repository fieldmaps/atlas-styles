{
  "name": "atlas-styles",
  "main": "index.js",
  "version": "1.0.0",
  "repository": "https://github.com/fieldmaps/styles",
  "author": "Max Malynowsky",
  "license": "MIT",
  "scripts": {
    "styles": "HOST=http://localhost:8080 ts-node src/styles",
    "tile-json": "HOST=http://localhost:8080 ts-node src/tile-json",
    "tile-labels": "src/tile-labels/run.sh",
    "tile-filter": "ts-node src/tiles/filter.ts",
    "tiles": "src/tiles/land.sh && src/tiles/adm-labels.sh && src/tiles/adm-lines.sh && src/tiles/tile-join.sh",
    "serve": "http-server dist",
    "rm": "s3cmd rm s3://fieldmapstiles/ --force --recursive",
    "put": "s3cmd put dist/* s3://fieldmapstiles/ --recursive --acl-public --exclude '.DS_Store'",
    "sync": "s3cmd sync dist/ s3://fieldmapstiles/ --delete-removed --acl-public --exclude '.DS_Store'",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "compress": "rm -f dist/tiles.tar.gz && cd dist && tar --exclude='*.DS_Store' -zcf tiles.tar.gz fonts sprites styles v4",
    "upload": "s3cmd put dist/tiles.tar.gz s3://fieldmapsdata/",
    "download": "curl https://data.fieldmaps.io/tiles.tar.gz --output atlas-styles.tar.gz",
    "extract": "tar -xf tiles.tar.gz --directory public && rm tiles.tar.gz",
    "build": "yarn download && mkdir -p public && yarn extract"
  },
  "dependencies": {
    "@turf/turf": "^5.1.6",
    "ndjson": "^2.0.0",
    "polylabel": "^1.1.0",
    "yargs": "^16.1.1"
  },
  "devDependencies": {
    "@types/ndjson": "^2.0.0",
    "@types/polylabel": "^1.0.5",
    "@types/yargs": "^15.0.11",
    "http-server": "^0.12.3",
    "prettier": "^2.2.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.2"
  }
}

{
  "name": "styles",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/fieldmaps/styles",
  "author": "Max Malynowsky <maxmalynowsky@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": "10"
  },
  "scripts": {
    "compress": "cd dist && tar --exclude='*.DS_Store' -zcf atlas-styles.tar.gz fonts sprites styles tiles",
    "extract": "tar -xf dist/atlas-styles.tar.gz --directory static",
    "fonts": "node src/fonts/index.js",
    "labels": "node src/tiles/labels.js",
    "sprites": "node src/sprites/index.js",
    "styles": "node src/styles/index.js",
    "sync": "aws s3 sync dist s3://fieldmaps-atlas --exclude '*.DS_Store'",
    "sync-rm": "aws s3 rm s3://fieldmaps-atlas --recursive --exclude '*' --include '*.DS_Store'",
    "tiles": "node src/tiles/index.js"
  },
  "dependencies": {
    "@mapbox/spritezero": "^6.2.0-dev",
    "@turf/turf": "^5.1.6",
    "d3-dsv": "^1.2.0",
    "fontnik": "^0.5.3",
    "glyph-pbf-composite": "^0.0.2",
    "polylabel": "^1.0.2",
    "tippecanoe": "^0.5.0"
  },
  "devDependencies": {
    "svgo": "^1.3.2"
  }
}
